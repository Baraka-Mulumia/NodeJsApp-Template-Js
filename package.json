        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "semver": "^7.3.7",
        "uuid": "^9.0.0",
        "validator": "^13.7.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
